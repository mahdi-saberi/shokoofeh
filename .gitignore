# Git ignore file for Shokoofeh project

# Environment files
.env
.env.*
docker-compose.env
!env.production.template

# Docker files
Dockerfile
docker-compose*.yml
.dockerignore

# Logs
*.log
logs/

# Cache
.cache/
*.cache

# Temporary files
*.tmp
*.temp

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Backup files
*.backup
*.tar.gz

# Node modules (if any)
node_modules/

# Build artifacts
build/
dist/

# Database files
*.sql
*.sqlite

# Storage (will be mounted as volume)
storage/framework/cache/*
storage/framework/sessions/*
storage/framework/views/*
storage/logs/*
!storage/framework/cache/.gitignore
!storage/framework/sessions/.gitignore
!storage/framework/views/.gitignore
!storage/logs/.gitignore

# Bootstrap cache (will be created in container)
bootstrap/cache/*
!bootstrap/cache/.gitignore 